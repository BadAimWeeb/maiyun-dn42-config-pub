[Unit]
Description=AMPR RIP44 Daemon
Wants=syslog.service local-fs.target network-online.target wait-address@44.63.16.201\x25tunl0.service
After=local-fs.target network-online.target sys-subsystem-net-devices-tunl0.device wait-address@44.63.16.201\x25tunl0.service

[Service]
Restart=always
ExecStart=/usr/local/sbin/ampr-ripd -d -s -i tunl0 -t 44 -a 44.63.16.192/29,44.63.16.200/29,44.61.18.210/28 -L AK6DS@DM12jv

[Install]
WantedBy=multi-user.target
