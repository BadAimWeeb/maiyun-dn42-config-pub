# vi: ft=systemd
# /etc/systemd/network/20-internal1-wg.network

[Match]
Name=wginternal1

[Link]
RequiredForOnline=degraded:routable
RequiredFamilyForOnline=ipv6
Group=4040

[Network]
Description=Internal WireGuard tunnel between ab06 and uc01
DHCP=no
IPv6AcceptRA=false
IPv4Forwarding=yes
IPv6Forwarding=yes
IPv4ReversePathFilter=no
KeepConfiguration=yes

[Address]
Address=fe80::2c01/64
